{
    "extends": "airbnb-base",
    "env": {
       "es6": true,
       "browser": true,
       "jest": true
    },
    "rules": {
      "no-restricted-syntax": [
        "error",
        "LabeledStatement",
        "WithStatement"
      ],
      "no-console": "off",
      "import/no-named-as-default": 0,
      "import/no-named-as-default-member": 0,
      "no-new": "off",
      "no-else-return": "off",
      "prefer-regex-literals": "off"
    },
    "parser": "@babel/eslint-parser"
  }